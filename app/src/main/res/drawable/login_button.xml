<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="100dp"
                android:height="100dp"/>
            <solid android:color="@color/black"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/forword_arrow"
        android:left="25dp"
        android:right="25dp"
        android:top="30dp"
        android:bottom="30dp">
        <shape>
            <size
                android:width="20dp"
                android:height="20dp"/>
        </shape>
    </item>
</layer-list>